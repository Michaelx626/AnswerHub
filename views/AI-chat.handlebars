<div class='container'>
  <div class='row'>
    <div class='col-md-12'>
      <h1>AnswerHub Chat</h1>
      <div class='messages-container' id='message-container'>
        <div class='message' id='messages'>
          <div class='sender'>AI Assistant</div>
          <div class='timestamp' id='timestamp'></div>
          <div class='content'>
            Hi there! What's your coding question?
          </div>
        </div>

      </div>
      <div class='input-area'>
        <input
          type='text'
          id='user-input'
          class='form-control'
          placeholder='Type a message...'
        />
        <button type='submit' id='sendBtn' class='btn btn-primary'><i
            class='fa fa-paper-plane'
          ></i></button>
      </div>
    </div>
  </div>
</div>
<script src='/js/ai-front-end.js'></script>